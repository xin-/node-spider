{
  "name": "node-spider",
  "version": "1.0.0",
  "description": "node spider",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d lib",
    "dev-es6": "babel-node ./dev-main.js",
    "dev-es5": "sudo /usr/local/redis-3.2.8/bin/redis-server /usr/local/redis-3.2.8/etc/redis.conf && nodemon ./dev-main-es5.js",
    "scripts-des": "dev-es5 命令中第一句是开启redis服务器",
    "webpack-build": "webpack",
    "webpack-dev": "webpack-dev-server",

    "lint": "eslint src",
    "test": "make test",
    "start": "npm run dev:local",
    "dev:local": "cross-env NODE_ENV=development node build/webpack.dev.js --hot",
    "dev:test": "cross-env NODE_ENV=development node build/webpack.dev.js --hot --deploy",
    "release": "npm run deploy:online",
    "deploy:test": "cross-env NODE_ENV=production node build/webpack.release.js --watch --uglify --deploy=test",
    "deploy:online": "cross-env NODE_ENV=production node build/webpack.release.js --uglify --deploy=online"
  },
  "author": "xinHonglin",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-classes": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.24.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.18.0",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "react": "^15.5.4",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "async": "^2.0.1",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "cesium": "^1.25.0",
    "cheerio": "^0.22.0",
    "express": "^4.15.2",
    "jade": "^1.11.0",
    "jquery": "^3.2.1",
    "morgan": "^1.8.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "redis": "^2.7.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "superagent": "^2.3.0"
  }
}
